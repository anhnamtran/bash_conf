#!/usr/bin/env bash
# Starts monero in the right location
pushd "$(op read "op://Personal/MoneroPrime/Access Keys/wallet_dir")" || exit 1
monero-wallet-cli \
  --wallet-file "$(op read "op://Personal/MoneroPrime/Access Keys/wallet_name")" \
  --password "$(op read "op://Personal/MoneroPrime/password")"
popd > /dev/null 2>&1 || exit 1
